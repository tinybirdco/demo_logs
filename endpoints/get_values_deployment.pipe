NODE endpoint_get_values_deployment
SQL >

    SELECT distinct deploymentId
    FROM
    (
      SELECT distinct deploymentId 
        FROM build_log
      UNION ALL
      SELECT distinct deploymentId
        FROM lambda_log
      UNION ALL
      SELECT distinct deploymentId
        FROM rewrite_log  
    )



NODE get_values_deployment_1
SQL >

    SELECT count() FROM endpoint_get_values_deployment


