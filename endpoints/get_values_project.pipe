DESCRIPTION >
	Retrive distinct values for Project Id


NODE get_distinct_values_project
SQL >

    %
    SELECT distinct projectId
      {% if not defined(channel) or channel == 'build' %}
      FROM build_log
      {% elif channel == 'lambda' %}
      FROM lambda_log
      {% elif channel == 'rewrite' %}
      FROM rewrite_log
      {% end %}



NODE endpoint_get_values_project
SQL >

    SELECT 'All' as projectId
    UNION ALL
    SELECT * FROM get_distinct_values_project


